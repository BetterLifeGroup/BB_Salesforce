public with sharing class SRVAFFPreapprovalController {

    @AuraEnabled
    public static String getPreApprovalDocument(String opportunityId, Boolean forceRegen) {
        System.debug('force regen' + forceRegen);
        System.debug('Opp id' + opportunityId);

        try {
            if (!forceRegen) {
                ContentVersion cv = [SELECT Id, ContentSize, FileType, VersionData, FirstPublishLocationId, CreatedDate FROM ContentVersion WHERE FirstPublishLocationId = :opportunityId AND Title LIKE '%Preapproval%' LIMIT 1];

                List<LoanApplicant> laList = [SELECT Id, LastModifiedDate FROM LoanApplicant WHERE IsActive__c = TRUE AND LoanApplicationId = :[SELECT Id FROM ResidentialLoanApplication WHERE OpportunityId = :opportunityId LIMIT 1].Id];
                List<LoanApplicantLiability> lal = [SELECT Id, LastModifiedDate FROM LoanApplicantLiability WHERE LoanApplicantId IN :laList];
                List<LoanApplicantIncome> lai = [SELECT Id, LastModifiedDate FROM LoanApplicantIncome WHERE LoanApplicantId IN :laList];

                Datetime latestChange = Datetime.newInstance(2023, 01, 01, 0, 0, 0);

                for (LoanApplicantIncome income : lai) {
                    if (income.LastModifiedDate > latestChange) {
                        latestChange = income.LastModifiedDate;
                    }
                }

                for (LoanApplicantLiability applicantLiability : lal) {
                    if (applicantLiability.LastModifiedDate > latestChange) {
                        latestChange = applicantLiability.LastModifiedDate;
                    }
                }

                for (LoanApplicant la : laList) {
                    if (la.LastModifiedDate > latestChange) {
                        latestChange = la.LastModifiedDate;
                    }
                }

                System.debug('latest- ' + latestChange + ' cv.Created- ' + cv.CreatedDate);
                if (latestChange < cv.CreatedDate) {
                    System.debug('returned current cert');
                    return EncodingUtil.base64Encode(cv.VersionData);
                }
            }
        } catch (Exception e) {

        }
        System.debug('created new cert');
        Opportunity opportunity = [
                SELECT Id, Name, OwnerId, ConsultantEmail__c, ConsultantBranch__r.Name,
                        ConsultantRegion__r.Name, ConsultantSubBranch__r.Name, Reference_Number_Text__c
                FROM Opportunity
                WHERE Id = :opportunityId
                LIMIT 1
        ];

        ResidentialLoanApplication rla = [
                SELECT Id, OpportunityId, Total_Gross_Income_Amount__c, Total_Contractual_Expenses_Amount__c,
                        Total_Liability_Amount__c, Deposit_1__c, Deposit_2__c, Deposit_3__c, Deposit_4__c, Loan_Term__c, Total_Income_Amount__c, Total_Deductions_Amount__c, Total_Nett_Income__c
                FROM ResidentialLoanApplication
                WHERE OpportunityId = :opportunity.Id
                LIMIT 1
        ];

        List<LoanApplicant> applicantList = [
                SELECT Id, Name, Personal_ID_Number__c, Bond_Settlement_Amount__c, Bond_Repayment_Amount__c, Is_Main_Applicant__c, Contact.Account.Identification_Number__c, Contact.Account.Passport_Number__c, Contact.Account.FirstName, Contact.Account.LastName
                FROM LoanApplicant
                WHERE LoanApplicationId = :rla.Id AND IsActive__c = TRUE
                ORDER BY Is_Main_Applicant__c DESC
                LIMIT 20
        ];

        User consultant = [
                SELECT Id, Name, Email, MobilePhone, Phone
                FROM User
                WHERE Id = :opportunity.OwnerId
                LIMIT 1
        ];

        SRVAFFCreatePreApprovalCertificateRe req = new SRVAFFCreatePreApprovalCertificateRe();

        req.templateTypeId = 1;
        SRVAFFSlug slug = new SRVAFFSlug();

        slug.branch = opportunity.ConsultantBranch__r.Name != null ? opportunity.ConsultantBranch__r.Name : 'Branch';
        slug.region = opportunity.ConsultantRegion__r.Name != null ? opportunity.ConsultantRegion__r.Name : 'Region';
        slug.consultantFullName = consultant.Name;
        slug.consultantTelephoneNumber = consultant.Phone != null ? consultant.Phone : '0800007111';
        slug.consultantCellNumber = consultant.MobilePhone != null ? consultant.MobilePhone : '';
        slug.consultantEmailAddress = opportunity.ConsultantEmail__c;
        req.slugs = slug;

        List<SRVAFFApplicants> affApplicants = new List<SRVAFFApplicants>();

        Double bondRepaymentToBeSettled = 0;
        Double existingBondRepayment = 0;

        for (LoanApplicant app : applicantList) {
            SRVAFFApplicants applicant = new SRVAFFApplicants();
            String str = app.Contact.Account.FirstName;

            str = str.trim();
            String initials = str.substring(0, 1);
            while (str.contains(' ')) {

                Integer index = str.indexOf(' ');
                initials = initials + str.substring(index + 1, index + 2);
                str = str.substring(index + 1);
            }
            initials = initials.replaceAll(' ', '');


            applicant.applicantFullName = initials + ' ' + app.Contact.Account.LastName;
//            applicant.applicantIdNumber = app.Personal_ID_Number__c;
            applicant.applicantIdNumber = app.Contact.Account.Identification_Number__c != null ? app.Contact.Account.Identification_Number__c : app.Contact.Account.Passport_Number__c;
            bondRepaymentToBeSettled += app?.Bond_Settlement_Amount__c != null ? app.Bond_Settlement_Amount__c : 0;
            existingBondRepayment += app?.Bond_Repayment_Amount__c != null ? app.Bond_Repayment_Amount__c : 0;

            affApplicants.add(applicant);
        }

        req.applicants = affApplicants;

        req.depositPercentage1 = Integer.valueOf(rla.Deposit_1__c);
        req.depositPercentage2 = Integer.valueOf(rla.Deposit_2__c);
        req.depositPercentage3 = Integer.valueOf(rla.Deposit_3__c);
        req.depositPercentage4 = Integer.valueOf(rla.Deposit_4__c);
        req.loanTermInYears = Integer.valueOf(rla.Loan_Term__c);
        req.certificateValidUntilInMonths = 3;
        req.grossIncome = rla.Total_Income_Amount__c;
        req.totalExpenses = Double.valueOf(rla.Total_Contractual_Expenses_Amount__c + rla.Total_Liability_Amount__c + rla.Total_Deductions_Amount__c);
        System.debug(rla.Total_Contractual_Expenses_Amount__c);
        System.debug(rla.Total_Liability_Amount__c);
        System.debug(rla.Total_Deductions_Amount__c);
        paCert__c includeSettlementAmount = paCert__c.getOrgDefaults();
        if (includeSettlementAmount.includeSettlementAmount__c == false && bondRepaymentToBeSettled > 0) {
            req.totalExpenses -= existingBondRepayment;
        }
        req.additionalPaymentPerMonthTotal = 0;
        req.bondRepaymentToBeSettled = bondRepaymentToBeSettled != null ? bondRepaymentToBeSettled : 0;
        req.existingBondRepayment = existingBondRepayment != null ? existingBondRepayment : 0;

        Map<String, Object> params = new Map<String, Object>{
                'preApprovalId' => String.valueOf(opportunity.Reference_Number_Text__c),
                'SRVAFFCreatePreApprovalCertificateRe' => req
        };

        SRVAFFClient client = new SRVAFFClient();
        SRVAFFPreApprovalApi api = new SRVAFFPreApprovalApi(client);
        Integration_Parameter__mdt integrationParam = Integration_Parameter__mdt.getInstance('Subscription_Key');
        ((OAS.ApiKeyAuth) client.getAuthentication('apiKeyHeader')).setApiKey(integrationParam.Parameter_Value__c);

        String base64String;
        try {
            base64String = api.postPreapprovalPreapprovalidCertificate(params);
        } catch (Exception e) {

            Case ca = [SELECT Id FROM Case WHERE Opportunity__c = :opportunityId ORDER BY LastModifiedDate DESC LIMIT 1];

            if (String.valueOf(e).toLowerCase().contains('gross income needs to be greater than 0')) {
                ca.Domain_Failure_Reason__c = 'Gross income needs to be greater than 0';
            } else if (String.valueOf(e).toLowerCase().contains('gross income needs to be greater than total expenses')) {
                ca.Domain_Failure_Reason__c = 'Gross income needs to be greater than total expenses';
            } else if (String.valueOf(e).toLowerCase().contains('affordability is less than r100000')) {
                ca.Domain_Failure_Reason__c = 'No pre-approval can be generated, affordability is less than R100000';
            } else if (String.valueOf(e).toLowerCase().contains('total expenses needs to be greater than 0')) {
                ca.Domain_Failure_Reason__c = 'Total expenses needs to be greater than 0';
            } else {
                ca.Domain_Failure_Reason__c = 'DOMAIN RESPONSE ERROR: Please contact support desk';
                System.debug('PA Exception not handled - ' + e);
            }
            update ca;

            opportunity.PACertPublicDownloadLink__c = '';
            opportunity.PACertPublicViewLink__c = '';

            update opportunity;

            List<ContentVersion> cvList = [SELECT Id, ContentDocumentId FROM ContentVersion WHERE Id = :opportunity.Id AND Title LIKE '%approval%'];
            List<String> docIds = new List<String>();
            for (ContentVersion cv : cvList) {
                docIds.add(cv.ContentDocumentId);
            }
            List<ContentDocument> cdLink = [SELECT Id FROM ContentDocument WHERE Id IN :docIds];
            delete cdLink;

            return 'error' + ca.Domain_Failure_Reason__c;
        }

        try {
            if (base64String != null) {

                SRVAFFCertificateDataApi affordabilityApi = new SRVAFFCertificateDataApi(client);
                ((OAS.ApiKeyAuth) client.getAuthentication('apiKeyHeader')).setApiKey(integrationParam.Parameter_Value__c);

                System.debug(JSON.serialize(req));
                Map<String, Object> affordabilityParams = new Map<String, Object>{
                        'SRVAFFCertificateDataApi' => req
                };

                SRVAFFCertificateDataResponse affordabilityResp = affordabilityApi.postCertificateData(affordabilityParams);
                System.debug(affordabilityResp);

                Decimal affordabilityAmount = Decimal.valueOf(affordabilityResp.purchasePrice.replaceAll('R','').replaceAll(' ',''));
//                if (!Test.isRunningTest()) {
//                    affordabilityAmount = SRVAFFPreapprovalController.GetAffordability((Double) req.grossIncome, (Double) rla.Total_Nett_Income__c, req.totalExpenses, (Double) req.existingBondRepayment - req.bondRepaymentToBeSettled);
//                } else {
//                    affordabilityAmount = 2211;
//                }
                update new Opportunity(Id = opportunityId, Wrap_Up_Reason__c = 'Automated', Affordability_Amount__c = affordabilityAmount);

                Datetime dt = System.Datetime.now();
                ContentVersion newCV = new ContentVersion();
                newCV.Title = 'Preapproval - ' + String.valueOf(dt);
                newCV.FirstPublishLocationId = opportunityId;
                newCV.VersionData = EncodingUtil.base64Decode(base64String);
                newCV.PathOnClient = 'preapproval.pdf';

                insert newCV;

            }

        } catch (OAS.ApiException ex) {
            System.debug(ex);
            return 'data:image/jpeg;base64,';
        }
        try {
            Case ca = [SELECT Id FROM Case WHERE Opportunity__c = :opportunityId ORDER BY LastModifiedDate DESC LIMIT 1];
            ca.Domain_Failure_Reason__c = '';

            update ca;
        } catch (Exception e) {
        }
        return base64String;

    }


    @AuraEnabled
    public static Boolean PaCertExists(String opportunityId) {

//        ResidentialLoanApplication rla = [SELECT Id FROM ResidentialLoanApplication WHERE OpportunityId = :opportunityId LIMIT 1];


        System.debug(opportunityId);

        Boolean paExists = false;

        try {
            ContentVersion cv = [SELECT Id, ContentSize, FileType, FirstPublishLocationId, CreatedDate FROM ContentVersion WHERE FirstPublishLocationId = :opportunityId AND Title LIKE '%Preapproval%' LIMIT 1];

            List<LoanApplicant> laList = [SELECT Id, LastModifiedDate FROM LoanApplicant WHERE IsActive__c = TRUE AND LoanApplicationId = :[SELECT Id from ResidentialLoanApplication where OpportunityId = :opportunityId limit 1].Id];
            List<LoanApplicantLiability> lal = [SELECT Id, LastModifiedDate FROM LoanApplicantLiability WHERE LoanApplicantId IN :laList];
            List<LoanApplicantIncome> lai = [SELECT Id, LastModifiedDate FROM LoanApplicantIncome WHERE LoanApplicantId IN :laList];

            Datetime latestChange = Datetime.newInstance(2023, 01, 01, 0, 0, 0);

            for (LoanApplicantIncome income : lai) {
                if (income.LastModifiedDate > latestChange) {
                    latestChange = income.LastModifiedDate;
                }
            }

            for (LoanApplicantLiability applicantLiability : lal) {
                if (applicantLiability.LastModifiedDate > latestChange) {
                    latestChange = applicantLiability.LastModifiedDate;
                }
            }

            for (LoanApplicant la : laList) {
                System.debug('latschangevar' + latestChange + ' - la.lastmodified - ' + la.LastModifiedDate);
                if (la.LastModifiedDate > latestChange) {
                    latestChange = la.LastModifiedDate;
                }
            }

            System.debug('latest- ' + latestChange + ' cv.Created- ' + cv.CreatedDate);
            if (latestChange > cv.CreatedDate) {
                return paExists;
            }

        } catch (Exception e) {
            return paExists;
        }

        paExists = true;

        return paExists;

    }

    public static Double GetInterestRate() {

        SRVAFFClient client = new SRVAFFClient();
        SRVAFFRatesApi api = new SRVAFFRatesApi(client);
        Integration_Parameter__mdt integrationParam = Integration_Parameter__mdt.getInstance('Subscription_Key');
        ((OAS.ApiKeyAuth) client.getAuthentication('apiKeyHeader')).setApiKey(integrationParam.Parameter_Value__c);

        String str = api.getGetinterestrate();

        Double interestRate = Double.valueOf(str);

        return interestRate;

    }

    public static Decimal GetAffordability(Double grossIncome, Double netIncome, Double totalExpenses, Double existingBondRepayments) {

        SRVAFFClient client = new SRVAFFClient();
        SRVAFFCalculatorApi api = new SRVAFFCalculatorApi(client);
        Integration_Parameter__mdt integrationParam = Integration_Parameter__mdt.getInstance('Subscription_Key');
        ((OAS.ApiKeyAuth) client.getAuthentication('apiKeyHeader')).setApiKey(integrationParam.Parameter_Value__c);

        SRVAFFAffordabilityRequestModel req = new SRVAFFAffordabilityRequestModel();

        req.grossMonthlyIncome = grossIncome;
        req.netMonthlyIncome = netIncome;
        req.roundData = true;
//        if (!Test.isRunningTest()) {
//            req.interestRate = SRVAFFPreapprovalController.GetInterestRate();
//        } else {
//            req.interestRate = 11;
//        }
        req.repaymentTermInYears = 20;
        req.totalMonthlyExpenses = totalExpenses;
        req.existingBondRepayments = existingBondRepayments;

        Map<String, Object> params = new Map<String, Object>{
                'SRVAFFAffordabilityRequestModel' => req
        };

        SRVAFFAffordabilityResultVm result = new SRVAFFAffordabilityResultVm();
        if (!Test.isRunningTest()) {
            result = api.postCalculateaffordability(params);
        } else {
            result = SRVAFFAffordabilityResultVm.getExample();
        }
        System.debug(result);

        return result.loanAmount;
    }

}