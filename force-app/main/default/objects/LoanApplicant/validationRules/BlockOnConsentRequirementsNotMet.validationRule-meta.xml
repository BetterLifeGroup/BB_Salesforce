<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BlockOnConsentRequirementsNotMet</fullName>
    <active>true</active>
    <errorConditionFormula>OR(AND(Contact.Account.ConsentRequirementsMet__c = false ,idvFinalScore__c != null),AND(Contact.Account.ConsentRequirementsMet__c = false ,creditCheckScore__c != null))</errorConditionFormula>
    <errorMessage>Consent Requirements Not Met</errorMessage>
</ValidationRule>
