<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>endDateCantBeInPast</fullName>
    <active>true</active>
    <errorConditionFormula>if(ISNULL(End_Date__c),false,End_Date__c &lt;  TODAY())</errorConditionFormula>
